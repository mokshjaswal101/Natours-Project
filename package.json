{
  "name": "natours-project",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mokshjaswal101/Natours-Project.git"
  },
  "author": "Moksh Jaswal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mokshjaswal101/Natours-Project/issues"
  },
  "homepage": "https://github.com/mokshjaswal101/Natours-Project#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1"
  },
  "dependencies": {}
}
